{
    "name": "elife/civi-contacts",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "guzzlehttp/guzzle": "^7.4",
        "guzzlehttp/promises": "^1.5",
        "laravel-zero/framework": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "eLife\\CiviContacts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\eLife\\CiviContacts\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["civi-contacts"]
}
