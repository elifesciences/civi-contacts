{
    "name": "elife/civi-contacts",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.5",
        "guzzlehttp/promises": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "eLife\\CiviContacts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\eLife\\CiviContacts\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.1.33"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "suggest": {
        "laravel-zero/framework": "Required to use the console (^5.8)."
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.6"
    }
}
