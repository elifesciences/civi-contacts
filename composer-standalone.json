{
    "name": "elife/civi-contacts-standalone",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.5",
        "guzzlehttp/promises": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "eLife\\CiviContacts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\eLife\\CiviContacts\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.1.33"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["civi-contacts"],
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.6"
    }
}
